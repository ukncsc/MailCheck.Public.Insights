  <data name="GetRawData" xml:space="preserve">
    <value>
    SELECT 
      count Count, 
      host_name HostName,
      host_provider HostProvider,
      bounce_reflector_blocklist_count BounceReflectorBlocklistCount, 
      end_user_blocklist_count EndUserBlocklistCount, 
      end_user_network_blocklist_count EndUserNetworkBlocklistCount, 
      hijacked_network_blocklist_count HijackedNetworkBlocklistCount, 
      malware_blocklist_count MalwareBlockListCount, 
      proxy_blocklist_count ProxyBlocklistCount,
      spam_source_blocklist_count SpamSourceBlocklistCount,
      suspicious_network_blocklist_count SuspiciousNetworkBlockListCount,
      p P,
      pct PCT,
      disposition Disposition,
      dkim Dkim,
      spf Spf,
      arc Arc,
      forwarded Forwarded,
      local_policy LocalPolicy,
      mailing_list MailingList,
      other_override_reason OtherOverrideReason,
      trusted_forwarder TrustedForwarder
    FROM insights.aggregate_report_record_enriched
    WHERE reverse_domain = @reverseDomain
    AND effective_date Between @startDate AND @endDate;
    </value>
  </data>
  <data name="GetRawSubdomainData" xml:space="preserve">
    <value>SELECT 
      count Count, 
      host_name HostName,
      host_provider HostProvider,
      bounce_reflector_blocklist_count BounceReflectorBlocklistCount, 
      end_user_blocklist_count EndUserBlocklistCount, 
      end_user_network_blocklist_count EndUserNetworkBlocklistCount, 
      hijacked_network_blocklist_count HijackedNetworkBlocklistCount, 
      malware_blocklist_count MalwareBlockListCount, 
      proxy_blocklist_count ProxyBlocklistCount,
      spam_source_blocklist_count SpamSourceBlocklistCount,
      suspicious_network_blocklist_count SuspiciousNetworkBlockListCount,
      p P,
      disposition Disposition,
      dkim Dkim,
      spf Spf,
      arc Arc,
      forwarded Forwarded,
      local_policy LocalPolicy,
      mailing_list MailingList,
      other_override_reason OtherOverrideReason,
      trusted_forwarder TrustedForwarder
    FROM insights.aggregate_report_record_enriched
    WHERE reverse_domain like CONCAT(@reverseDomain, '.%')
    AND effective_date Between @startDate AND @endDate;</value>
